{
  "name": "rit-bikeshare-admin",
  "version": "0.1.6",
  "private": true,
  "dependencies": {
    "immutable": "^3.8.2",
    "moment": "^2.20.1",
    "prop-types": "^15.6.1",
    "q": "^1.5.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-google-maps": "^9.4.5",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^4.0.8",
    "react-scripts": "1.1.1",
    "recompose": "^0.26.0",
    "redux": "^3.7.2",
    "redux-actions": "^2.2.1",
    "redux-thunk": "^2.2.0",
    "semantic-ui-react": "^0.78.3",
    "styled-components": "^3.1.6"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "eslint": "^3.17.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-react": "^7.7.0",
    "husky": "^0.14.3",
    "lint-staged": "^4.3.0",
    "node-sass-chokidar": "^0.0.3",
    "npm-run-all": "^4.1.2",
    "prepush": "^3.1.11",
    "prettier": "^1.7.4"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/styles -o src/styles/css",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/styles -o public/styles/ --watch --recursive",
    "start-js": "PORT=80 react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "lint:all": "eslint . --fix",
    "lint:changed:js": "npm run changed:js --silent | xargs eslint",
    "precommit": "lint-staged",
    "prepush": "npm run lint:changed:js",
    "prettier": "prettier --single-quote --trailing-comma es5",
    "prettier:verify": "sh bin/prettier-verify.sh",
    "prettier:write": "npm run prettier -- --write \"static/**/*.js\"",
    "fix-githooks": "prepush -c package.json remove && node node_modules/husky/bin/install"
  },
  "lint-staged": {
    "*.js": [
      "eslint",
      "prettier -l"
    ]
  },
  "prepush": {
    "tasks": [
      "npm run lint-all"
    ]
  }
}
